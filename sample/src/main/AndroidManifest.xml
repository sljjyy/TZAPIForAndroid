<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tianzunchina.sample">

    <application
        android:name=".app.SysApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".event.EventActivity"
            android:label="@string/title_activity_event"/>
        <activity android:name=".form.DynamicFormActivity" />
        <activity android:name="com.tianzunchina.android.api.widget.photo.PreviewActivity" />
    </application>

</manifest>