<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">?attr/editTextColor</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.Sample" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimary">@color/light_blue_600</item>
        <item name="android:colorForeground">@color/background_material_dark</item>
        <item name="android:textColorPrimary">@color/background_material_dark</item>
        <item name="colorAccent">@android:color/holo_blue_light</item>
        <item name="colorPrimaryDark">@color/app_background_bule</item>
        <item name="android:textColorSecondary">@color/background_material_dark</item>
        <item name="android:textColorSecondaryInverse">@color/background_material_dark</item>
    </style>
    <style name="TitleListTextFont">
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">@color/text_333</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TitleTextFontChoose">
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">#A4000000</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TitleTextFont1">
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">#A4000000</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TitleTextFont">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#A4000000</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="operateButtonAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/operate_button_in</item>
        <item name="android:windowExitAnimation">@anim/operate_button_out</item>
    </style>

    <style name="TitleRemindText">
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TitleListTextFont2">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/text_333</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TitleTextFont3">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#A4000000</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ValueTextFont">
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">#42a370</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ValueTextFont2">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#42a370</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ValueTextNumber">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#FF0000</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ValueTextNumberHpCaseListFrg">
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">#FF0000</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TableTrMargin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginBottom">5dp</item>

    </style>
    <style name="AnimationFade">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="MyDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item><!--背景透明-->
        <item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">true</item><!--半透明-->
        <item name="android:windowContentOverlay">@null</item><!--内容覆盖 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item><!-- 窗口样式Dialog -->
        <item name="android:backgroundDimEnabled">true</item><!--模糊-->
        　</style>
    <style name="processDialog" >
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowBackground">@android:color/transparent</item><!--背景透明-->
        <item name="android:backgroundDimEnabled">true</item><!--模糊-->
        <item name="android:backgroundDimAmount">0.5</item>
        <item name="android:alpha">0.3</item>

    </style>
    <style name="circleActCommentLayoutAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/circle_act_comment_layout_in</item>
        <item name="android:windowExitAnimation">@anim/circle_act_comment_layout_out</item>
    </style>


    <style name="Widget.Design.AppBarLayout" parent="android:Widget">
        <item name="elevation">0dp</item>
        <item name="android:color">@color/white</item>
        <item name="android:background">?attr/colorPrimary</item>
    </style>

</resources>
