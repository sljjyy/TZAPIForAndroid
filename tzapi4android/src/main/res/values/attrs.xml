<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="quality" format="integer" />
    <attr name="groupCode" format="integer" />
    <attr name="boxCount" format="integer" />
    <attr name="boxWidth" format="dimension" />
    <attr name="boxHeight" format="dimension" />
    <attr name="boxIsLinear" format="boolean" />
    <attr name="boxLine" format="integer" />
    <attr name="boxColumn" format="integer" />
    <attr name="boxIndex" format="integer" />
    <attr name="isCompress" format="boolean" />
    <attr name="title" format="string" />
    <attr name="hint" format="string" />
    <declare-styleable name="TZPhotoBoxGroup">
        <attr name="quality"/>
        <attr name="groupCode" />
        <attr name="boxCount" />
        <attr name="boxWidth" />
        <attr name="boxHeight" />
        <attr name="boxIsLinear" />
        <attr name="boxLine" />
        <attr name="boxColumn" />
        <attr name="isCompress" />
    </declare-styleable>
    <declare-styleable name="TZPhotoBoxOne">
        <attr name="boxWidth" />
        <attr name="boxHeight" />
        <attr name="boxIndex" />
        <attr name="isCompress" />
    </declare-styleable>
    <declare-styleable name="AutofitTextView">
        <attr name="minTextSize" format="dimension" />
        <!-- Amount of precision used to calculate the correct text size to fit within its
        bounds. Lower precision is more precise and takes more time. -->
        <attr name="precision" format="float" />
        <!-- Defines whether to automatically resize text to fit to the view's bounds. -->
        <attr name="sizeToFit" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FormItemView">
        <attr name="title" />
        <attr name="hint" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="contentTextSize" format="dimension"/>
        <attr name="contentTextColor" format="color"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="expandText" format="string"/>
        <attr name="collapseDrawable" format="reference"/>
        <attr name="collapseText" format="string"/>
        <attr name="expandCollapseTextColor" format="color"/>
        <attr name="DrawableAndTextGravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>
</resources>
